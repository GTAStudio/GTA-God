{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "inbounds": [
    {
      "type": "anytls",
      "tag": "anytls-in",
      "listen": "127.0.0.1",
      "listen_port": {{ANYTLS_PORT}},
      "tcp_fast_open": true,
      "tcp_multi_path": {{TCP_MULTI_PATH}},
      "users": [
        {
          "name": "{{ANYTLS_USER}}",
          "password": "{{ANYTLS_PASSWORD}}"
        }
      ],
      "padding_scheme": [
        "stop=8",
        "0=30-30",
        "1=100-400",
        "2=400-500,c,500-1000,c,500-1000,c,500-1000,c,500-1000",
        "3=9-9,500-1000",
        "4=500-1000",
        "5=500-1000",
        "6=500-1000",
        "7=500-1000"
      ],
      "tls": {
        "enabled": true,
        "server_name": "{{ANYTLS_SNI}}",
        "min_version": "1.3",
        "kernel_tx": true,
        "certificate_path": "/data/caddy/certificates/{{ACME_CA_DIR}}/wildcard_.{{DOMAIN}}/wildcard_.{{DOMAIN}}.crt",
        "key_path": "/data/caddy/certificates/{{ACME_CA_DIR}}/wildcard_.{{DOMAIN}}/wildcard_.{{DOMAIN}}.key"
      }
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    }
  ]
}
